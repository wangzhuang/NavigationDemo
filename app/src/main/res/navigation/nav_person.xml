<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_person_fragment"
    android:label="second"
    app:startDestination="@+id/nav_person_fragment"
    tools:ignore="UnusedNavigation">
<fragment
    android:label="second"
    android:id="@+id/nav_person_fragment"
    android:name="com.king.navigationdemo.fragments.PersonFragment"
    tools:layout="@layout/fragment_person"
    >

    <action
        android:id="@+id/action_nav_person_fragment"
        app:destination="@+id/nav_person_fragment"
        app:enterAnim="@anim/nav_default_enter_anim"
        app:exitAnim="@anim/nav_default_exit_anim"
        app:popEnterAnim="@anim/nav_default_pop_enter_anim"
        app:popExitAnim="@anim/nav_default_pop_exit_anim" >
        <argument
            android:name="name"
            android:defaultValue="default_value" />
    </action>
</fragment>
</navigation>